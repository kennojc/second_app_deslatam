				</div>
    </div>

      <!-- Main -->
				<div class="wrapper style2">
				
        <%= yield %>	
        
				</div>

    <!-- Footer -->
  
      <div id="footer">
					<div class="container">
          <!-- Copyright -->
            <div class="copyright">
              <ul class="menu">
