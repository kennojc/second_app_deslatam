    <%= stylesheet_link_tag 'main', media: 'all', 'data-turbolinks-track': 'reload' %>
  </head>


  <body class="homepage is-preload">
  <style> 
                body {
                  background-image: url("<%= asset_path('header.jpg') %>");
  
                      }
</style>
    <div id="page-wrapper">
      <!-- Header -->
				<div id="header">
        
        <!-- Inner -->
						<div class="inner">
							<header>
								<h1>Second App Desaf√≠oLatam</h1>
							</header>
						</div>
          <!-- Nav -->
						<nav id="nav">
							<ul>
                <li><%= link_to 'Inicio' , root_path %></li>
                <li><%= link_to 'Test' , pages_test_path %></li>
